// Инициализирует структуру с параметрами события ПМ "ПослеЗаполненияКонтента"
//
// Параметры:
//  Контент			 - Структура	 - протоконтент;
//  ТипКонтента		 - Строка		 - идентификатор типа контента (см. ТипыКонтента())
//  СсылкаНаДокумент - ЛюбаяСсылка	 - ссылка на документ информационной базы;
// 
// Возвращаемое значение:
//  Структура - см. тело функции.
//
Функция ПараметрыЗаполненияКонтентаДляПМ(Контент, ТипКонтента, СсылкаНаДокумент)
	
	Результат = Новый Структура;
	Результат.Вставить("Content", Контент);
	Результат.Вставить("ТипКонтента", ТипКонтента);
	Результат.Вставить("ДокументСсылка", СсылкаНаДокумент);
	
	// Необязательные параметры
	Результат.Вставить("ФункцияУПД"); // Функция УПД/УКД для XML
	Результат.Вставить("ПараметрыОтправкиНаСогласование"); // Устарел, для совместимости со старыми ПМ
	Результат.Вставить("НомерЗаказа"); // Устарел, всегда Неопределено.
	Результат.Вставить("ДопСведения");
	Результат.Вставить("DocumentType"); // Устарел, для совместимости с ПМ 1.4 или ниже.
	Результат.Вставить("ПараметрыФормированияФайла");
	
	Возврат Результат;
	
КонецФункции

// Определяет значение параметра "DocumentType" события "ПослеЗаполненияКонтента".
//
// Параметры:
//  ТипКонтента - Строка - идентификатор типа контента (см. ТипыКонтента).
// 
// Возвращаемое значение:
//  Строка, Неопределено.
//
Функция ЗначениеDocumentTypeДляПМ(ТипКонтента)
	
	ВсеТипыКонтента = ТипыКонтента();
	
	Если ТипКонтента = ВсеТипыКонтента.UtdSellerContent Тогда
		Результат = "XmlUTD";
	ИначеЕсли ТипКонтента = ВсеТипыКонтента.UcdSellerContent Тогда
		Результат = "XmlUCD";
	ИначеЕсли ТипКонтента = ВсеТипыКонтента.Torg12SellerContent Тогда
		Результат = "XmlTorg12";
	ИначеЕсли ТипКонтента = ВсеТипыКонтента.AcceptanceCertificateSellerContent Тогда
		Результат = "XmlAcceptanceCertificate";
	ИначеЕсли ТипКонтента = ВсеТипыКонтента.InvoiceContent Тогда
		Результат = "Invoice";
	ИначеЕсли ТипКонтента = ВсеТипыКонтента.InvoiceCorrectionContent Тогда
		Результат = "InvoiceCorrection";
	Иначе
		Результат = Неопределено;
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

// Вызывает обработчик события ПМ "ПослеЗаполненияКонтента".
//
// Параметры:
//  Контент - Структура - протоструктура с описанием контента.
//  ОписаниеФормата - Структура - полное описание формата отправки.
//  СсылкаНаОбъект - ЛюбаяСсылка - документ информационной базы, на основании которого собирается контент.
//  ПараметрыФормирования - Структура - см. ДополнительныеПараметрыПолученияКонтента.
//
Процедура ПМ_ПослеЗаполненияКонтента(Контент, ОписаниеФормата, СсылкаНаОбъект, ПараметрыФормирования)
	
	ТипКонтента = ОписаниеФормата.ТипКонтента;
	
	ПараметрыПМ = ПараметрыЗаполненияКонтентаДляПМ(Контент, ТипКонтента, СсылкаНаОбъект);
	ПараметрыПМ.ПараметрыФормированияФайла = ПараметрыФормирования.ПараметрыФормированияФайла;
	ПараметрыПМ.DocumentType = ЗначениеDocumentTypeДляПМ(ТипКонтента);
	ПараметрыПМ.ДопСведения = ПараметрыФормирования.ДополнительныеСведенияСтрокой;
	ПараметрыПМ.ФункцияУПД = ПараметрыФормирования.ФункцияДокумента;
	
	ИмяСобытияПМ = "ПослеЗаполненияКонтента";
	ОбработкаСобытияПодключаемогоМодуля(ИмяСобытияПМ, ПараметрыПМ);
	
КонецПроцедуры

Процедура ПМ_ПослеЗаполненияНовогоДокумента(ДокументОбъект, Контент)
	
	ПараметрыПМ = Новый Структура;
	ПараметрыПМ.Вставить("ДокументОбъект"	, ДокументОбъект);
	ПараметрыПМ.Вставить("СтруктураКонтента", Контент);
	ПараметрыПМ.Вставить("BoxID"			, ""); // Для совместимости
	ПараметрыПМ.Вставить("DocumentID"		, ""); // Для совместимости
	
	ИмяСобытияПМ = "ПослеЗаполненияНовогоДокументаПоступления";
	ОбработкаСобытияПодключаемогоМодуля(ИмяСобытияПМ, ПараметрыПМ);
	
КонецПроцедуры

Процедура ЗаполнитьОрганизациюИнформационнойБазы(РеквизитыОрганизации, РеквизитыПодразделения)
	
	Если ЗначениеЗаполнено(РеквизитыОрганизации.Ссылка) Тогда
		Возврат;
	КонецЕсли;
	
	ИдентификаторОрганизации = РеквизитыОрганизации.ИдентификаторОрганизации;
	
	ПодразделениеКПП = "";
	Если ЗначениеЗаполнено(РеквизитыПодразделения) Тогда
		ПодразделениеКПП = РеквизитыПодразделения.КПП;
	КонецЕсли;
	
	РеквизитыОрганизации.Ссылка = МетодСервера(, "BoxID_2_Организация"
		, ИдентификаторОрганизации
		, ПодразделениеКПП);
	
КонецПроцедуры

Процедура ЗаполнитьКонтрагентаИнформационнойБазы(РеквизитыОрганизации)
	
	Если ЗначениеЗаполнено(РеквизитыОрганизации.Ссылка) Тогда
		Возврат;
	КонецЕсли;
	
	ИдентификаторОрганизации = РеквизитыОрганизации.ИдентификаторОрганизации;
	
	РеквизитыОрганизации.Ссылка = МетодСервера(, "CounteragentBoxID_2_Контрагент"
		, ИдентификаторОрганизации);
	
КонецПроцедуры

